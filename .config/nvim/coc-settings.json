{
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "terraform": {
      "command": "~/.bin/terraform-lsp",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "groovy": {
      "command": "java",
      "args": [
        "-jar",
        "~/Repos/groovy-language-server/build/libs/groovy-language-server-all.jar"
      ],
      "filetypes": ["groovy"]
    }
  }
}
